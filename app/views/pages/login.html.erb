<%= form_tag login_path, :id => 'login' do %>
  
  <%= text_field_tag :email, nil, placeholder: "email", size: 27, :class => 'email' %>
  <%= password_field_tag :password, nil, placeholder: "password", size: 27, :class => 'password' %>
  <%= submit_tag "User Log In" %>
  <% end %>

<%= form_tag photologin_path, :id => 'photog_login' do %>
  
